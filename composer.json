{
    "name": "rela589n/doctrine-event-sourcing",
    "description": "Package for simple event-sourcing implementation utilizing Doctrine ORM",
    "version": "1.0.0",
    "type": "library",
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "beberlei/doctrineextensions": "^1.3",
        "nesbot/carbon": "^2.46",
        "vlucas/phpdotenv": "^5.3",
        "symfony/var-dumper": "^5.2",
        "ramsey/uuid-doctrine": "^1.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "rela589n",
            "email": "zsidelnik@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"Rela589n\\DoctrineEventSourcing\\" : "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
